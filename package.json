{
  "name": "lakotiy-backend",
  "version": "2.19.8",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build:store": "cross-env NODE_ENV=production nx build store --configuration=production",
    "build:admin": "cross-env NODE_ENV=production nx build admin --configuration=production",
    "build:libs": "nx build libs --configuration=production",
    "build:all": "nx run-many --target=build --all --configuration=production",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start:dev:admin": "cross-env NODE_ENV=development nx serve admin --configuration=development",
    "start:prod:admin": "node dist/apps/admin/main",
    "start:dev:store": "cross-env NODE_ENV=development nx serve store --configuration=development",
    "start:prod:store": "node dist/apps/store/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prisma:generate": "prisma generate --schema=libs/prisma-schema/prisma/schema.prisma",
    "prisma:migrate": "dotenv -e apps/admin/.env.development -- prisma migrate dev --schema=libs/prisma-schema/prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema=libs/prisma-schema/prisma/schema.prisma",
    "prisma:seed": "dotenv -e apps/admin/.env.development -- prisma db seed"
  },
  "dependencies": {
    "@eslint/js": "^9.22.0",
    "@nestjs/axios": "^4.0.0",
    "@nestjs/common": "^11.0.7",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.0.7",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.7",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/serve-static": "^5.0.1",
    "@nestjs/swagger": "^11.0.3",
    "@nestjs/throttler": "^6.4.0",
    "axios": "^1.7.9",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.7",
    "cross-env": "^7.0.3",
    "cuid": "^3.0.0",
    "dotenv-cli": "^8.0.0",
    "globals": "^16.0.0",
    "ioredis": "^5.6.1",
    "luxon": "^3.6.1",
    "multer": "^1.4.5-lts.1",
    "nest-winston": "^1.10.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "qs": "^6.14.0",
