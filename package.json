    "eslint-config-prettier": "10.1.2",
    "eslint-plugin-prettier": "^5.2.5",
    "i": "^0.3.7",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "jsonc-eslint-parser": "^2.1.0",
    "libs": "^0.1.2",
    "npm": "^11.2.0",
    "nx": "20.8.0",
    "prettier": "^3.4.2",
    "prisma": "^6.10.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
